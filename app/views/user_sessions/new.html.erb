<div class="content_container">
<% form_for(@user_session) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.submit 'Log in' %>
  </p>
<% end %>
<!-- END .content_container --></div>